private val _userData: UserDataMapImpl = UserDataMapImpl()

@KtGramInternal
public object __CtxUtils : CtxUtils {
  override val isClassDataInitialized: Lazy<Unit> = lazy { Unit }

  override suspend fun clearClassData(tgId: Long) {
    _classData.clearAll(tgId)
  }
}

@OptIn(KtGramInternal::class)
private val _classData: ClassDataImpl =
    ClassDataImpl().also { __CtxUtils.isClassDataInitialized.value }

public val TelegramBot.userData: UserDataMapImpl
  get() = _userData

public val TelegramBot.classData: ClassDataImpl
  get() = _classData

public operator fun User.`get`(key: String): String? = _userData[id, key]

public operator fun User.`set`(key: String, `value`: String): Unit = _userData.set(id, key, value)

public suspend inline fun <reified S : TestWizard.NameStep> WizardContext.getState(step: TestWizard.NameStep? = null): String? {
  val engine = bot.update.registry.getActivity(currentWizardId) as? WizardActivity ?: return null
  val manager = engine.getStateManagerForStep(TestWizard.NameStep::class, bot) as? WizardStateManager<String?> ?: return null
  return manager.get(TestWizard.NameStep::class, userReference)
}

public suspend inline fun <reified S : TestWizard.NameStep> WizardContext.setState(`value`: String, step: TestWizard.NameStep? = null) {
  val engine = bot.update.registry.getActivity(currentWizardId) as? WizardActivity ?: return
  val manager = engine.getStateManagerForStep(TestWizard.NameStep::class, bot) as? WizardStateManager<String?> ?: return
  manager.set(TestWizard.NameStep::class, userReference, value)
}

public suspend inline fun <reified S : TestWizard.NameStep> WizardContext.delState(step: TestWizard.NameStep? = null) {
  val engine = bot.update.registry.getActivity(currentWizardId) as? WizardActivity ?: return
  val manager = engine.getStateManagerForStep(TestWizard.NameStep::class, bot) as? WizardStateManager<String?> ?: return
  manager.del(TestWizard.NameStep::class, userReference)
}

