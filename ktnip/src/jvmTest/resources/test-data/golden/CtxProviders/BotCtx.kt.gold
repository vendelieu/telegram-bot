package eu.vendeli.tgbot.generated

import TestProviderClass
import TestProviderUser
import eu.vendeli.tgbot.TelegramBot
import eu.vendeli.tgbot.annotations.`internal`.KtGramInternal
import eu.vendeli.tgbot.types.User
import eu.vendeli.tgbot.utils.common.CtxUtils
import kotlin.Int
import kotlin.Lazy
import kotlin.Long
import kotlin.OptIn
import kotlin.String
import kotlin.Unit

private val _userData: TestProviderUser = TestProviderUser()

@KtGramInternal
public object __CtxUtils : CtxUtils {
  override val isClassDataInitialized: Lazy<Unit> = lazy { Unit }

  override suspend fun clearClassData(tgId: Long) {
    _classData.clearAll(tgId)
  }
}

@OptIn(KtGramInternal::class)
private val _classData: TestProviderClass =
    TestProviderClass().also { __CtxUtils.isClassDataInitialized.value }

public val TelegramBot.userData: TestProviderUser
  get() = _userData

public val TelegramBot.classData: TestProviderClass
  get() = _classData

public operator fun User.`get`(key: String): Int? = _userData[id, key]

public operator fun User.`set`(key: String, `value`: Int): Unit = _userData.set(id, key, value)
